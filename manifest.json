{
  "manifest_version": 2,
  "name": "Bookwin",
  "version": "0.1.0",

  "description": "Moves window tabs to a bookmarks folder.",

  "icons": {
    "48": "icons/bookwin-48.png"
  },

  "permissions": [
    "bookmarks",
    "contextMenus",
    "tabs"
  ],

  "background": {
    "scripts": ["bookwin.js"]
  }
}
